<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.controller.ServerNotFoundController">
    <children>
        <Label layoutX="49.0" layoutY="73.0" prefHeight="107.0" prefWidth="503.0" style="-fx-background-color: #ccbfbf;"
               text="The server is currently unavailable. Please try again later..." textFill="#c31717">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Button fx:id="ok_button" onMouseClicked="#deleteServernotFoundModalFromWindow" layoutX="210.0" layoutY="209.0" mnemonicParsing="false" prefHeight="89.0" prefWidth="182.0" text="Ok"
                textFill="#c31717">
            <font>
                <Font name="System Bold" size="27.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
